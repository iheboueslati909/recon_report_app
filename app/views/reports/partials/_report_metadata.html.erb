<div class="page-break">
  <div class="report-section text-center">
    <h3 class="heading-secondary">Report Metadata</h3>
    <table class="table-meta">
      <tr>
        <td class="table-cell p-2"><strong>Version:</strong></td>
        <td class="table-cell text-regular"><%= @report_data.dig(:report_metadata, :report_version) || "N/A" %></td>
      </tr>
      <tr>
        <td class="table-cell p-2"><strong>Generated By:</strong></td>
        <td class="table-cell text-regular"><%= @report_data.dig(:report_metadata, :generated_by) || "N/A" %></td>
      </tr>
      <tr>
        <td class="table-cell p-2"><strong>Generated At:</strong></td>
        <td class="table-cell text-regular"><%= @report_data.dig(:report_metadata, :generated_at) || "N/A" %></td>
      </tr>
    </table>
  </div>
</div>
