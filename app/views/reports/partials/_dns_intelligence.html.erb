<div class="page-break">
  <% dns_info = @report_data.dig(:findings, :domain_dns_intelligence) || {} %>
  <% dns_summary = dns_info[:dns_records_summary] || {} %>
  <% whois = dns_info[:whois_info_shown] || [] %>

  <% if dns_info.present? %>
    <div class="report-section">
      <h3 class="heading-secondary text-center">Domain & DNS Intelligence</h3>
      
      <%= render 'reports/partials/domain_info', dns_info: dns_info %>
      <%= render 'reports/partials/dns_records', dns_summary: dns_summary %>
      <%= render 'reports/partials/whois_info', whois: whois, whois_note: dns_info[:whois_note] %>
    </div>
  <% end %>
</div>
