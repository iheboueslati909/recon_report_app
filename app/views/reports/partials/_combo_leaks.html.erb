<div class="page-break">
  <h4 class="heading-tertiary mt-7">Combo List Leaks</h4>
  <table class="table-base mt-3">
    <thead>
      <tr class="table-header">
        <th class="table-cell">Domain</th>
        <th class="table-cell">Email</th>
        <th class="table-cell">Password</th>
        <th class="table-cell">Year</th>
      </tr>
    </thead>
    <tbody>
      <% leaks[:shown].each do |entry| %>
        <tr>
          <td class="table-cell"><%= entry[2] %></td>
          <td class="table-cell"><%= entry[0] %></td>
          <td class="table-cell"><%= entry[1] %></td>
          <td class="table-cell"><%= entry[3] %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <% if leaks[:note].present? %>
    <p class="text-small text-muted mt-2"><%= leaks[:note] %></p>
  <% end %>
</div>
